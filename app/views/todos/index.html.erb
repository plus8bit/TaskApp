<h1>Todos#index</h1>
<p>Find me in app/views/todos/index.html.erb</p>

<% @todos.each do |f| %>
  <%= f.title %> <br>
  <%= f.description %><br>
  <%= f.user.email %><br>
  <%= link_to "Delete", user_todo_path(current_user, f), method: :delete %>
<% end %>


<%= link_to "New Todo", new_user_todo_path(current_user, @todo) %>
<%= link_to "Sign Out", destroy_user_session_path, method: :delete %>


